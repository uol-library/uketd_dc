  <xs:complexType name="RFC1766">
   <xs:simpleContent>
    <xs:restriction base="dc:SimpleLiteral">
        <xs:simpleType>
          <xs:restriction base="xs:language"/>
        </xs:simpleType>
        <xs:attribute ref="xml:lang" use="prohibited"/>
